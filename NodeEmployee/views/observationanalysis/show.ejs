	<script type="text/javascript">

 
 $( document ).ready(function() {
   
	 var dt1 = '<%= observationanalysis.reviewdate %>';
	 d1 = new Date(dt1);
	 $('#reviewdate').val($.datepicker.formatDate('yy-mm-dd', d1));
	
    <% for (var j = 0 ; j < observationanalysis.nonationality ;j++){ %>
	
	var element = document.createElement("input");
	element.setAttribute("type", "text");
    element.setAttribute("value", "<%= observationanalysis.nationality[j] %>");
	element.setAttribute("class", "form-control");
	element.setAttribute("disabled","disabled");
	var item = document.getElementById('select-container');
	item.appendChild(element);
     <%}%>
    
  
  
  
  
});



</script>							
						
			
			